void Adding(int Value, int Iterations) {
    print("Beggingin value")
    print(Value)

    for(int Iter = 0; Iter < Iterations; Iter = Iter + 1) {
        Value = Value + 1
    }

    print("Endggingin value")
    print(Value)
}

int Main () {

    int MyInt

    read(MyInt)

    int Resoult = 0

    print("Beggingin result")
    print(Resoult)

    Adding(Resoult, MyInt)

    print("Endggingin result")
    print(Resoult)
    
    return 0
}